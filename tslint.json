{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": ["single", "avoid-escape"],
      "no-console": false,
      "trailing-comma": {
        "multiline": "never",
        "singleline": "never"
      },
      "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
